{
  "name": "@piopon/web-scraper",
  "version": "1.1.0",
  "description": "Scrap publicly available data from different web pages",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index",
    "dev": "nodemon src/index",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --silent --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piopon/web-scraper.git"
  },
  "keywords": [
    "data scraping",
    "web scraping"
  ],
  "author": "piopon <piopon.github@gmail.com>",
  "license": "GPL-2.0",
  "dependencies": {
    "@piopon/web-scraper": "file:",
    "ajv": "^8.17.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "express-flash": "^0.0.2",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.1",
    "handlebars-helpers": "^0.10.0",
    "jsonwebtoken": "^9.0.2",
    "locate-chrome": "^0.1.1",
    "moment": "^2.30.1",
    "mongoose": "^8.9.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "puppeteer": "^24.1.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  },
  "jest": {
    "slowTestThreshold": 15,
    "transform": {},
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 80,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "exhortignore": [
    "web-scraper"
  ]
}
